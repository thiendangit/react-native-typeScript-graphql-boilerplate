{
  "env": {
    "production": {
      "plugins": [
        "transform-remove-console"
      ]
    }
  },
  "plugins": [
    [
      "module-resolver",
      {
        "root": [
          "./"
        ],
        "alias": {
          "@src": "./src",
          "@utils": "./src/lib/utils",
          "@screens": "./src/screens",
          "@common": "./src/common",
          "@navigation": "./src/navigation",
          "@components": "./src/components",
          "@lib": "./src/lib",
          "@assets": "./src/assets",
          "@themes": "./src/themes",
          "@containers": "./src/containers",
          "@models": "./src/models"
        }
      }
    ]
  ]
}
